    "ng-packagr": "^5.3.0",
    "protractor": "~6.0.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.36.0",
    "tslib": "^1.9.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
