{
  "name": "ui-lib",
  "version": "1.0.0",
  "description": "UI library featuring various angular based components",
  "scripts": {
    "build": "npm run test-server && npm run lint && npm run build-script",
    "build-only": "npm run bumpVersion && npm run build-script",
    "start": "gulp watch",
    "build-script": "ng build ng-library --prod && gulp",
    "test": "ng test ng-library",
    "test-server": "ng test ng-library --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint ng-library",
    "gulp": "gulp",
    "bumpVersion": "gulp bump",
    "deploy": "gh-pages -d dist/ui-library-documentation",
    "test-coverage": "ng test ng-library --codeCoverage=true --watch=false --browsers=ChromeHeadless && cat ./coverage/lcov.info | codacy-coverage -t 8863a82776d04472bf5173b2fe6c5f0b"
  },
  "author": "Tushar Nimje",
  "license": "MIT",
  "engines": {
    "node": ">=10.10.0",
    "npm": ">=6.6.0"
  },
  "dependencies": {
    "@angular/animations": "9.1.1",
    "@angular/cdk": "9.2.4",
    "@angular/common": "9.1.1",
    "@angular/compiler": "9.1.1",
    "@angular/core": "9.1.1",
    "@angular/forms": "9.1.1",
    "@angular/platform-browser": "9.1.1",
    "@angular/platform-browser-dynamic": "9.1.1",
    "@angular/router": "9.1.1",
    "core-js": "3.6.5",
    "font-awesome": "4.7.0",
    "grid-styled-css": "1.0.2",
    "primeicons": "4.0.0",
    "primeng": "9.1.3",
    "rxjs": "6.6.3",
    "tslib": "1.13.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.12",
    "@angular-devkit/build-ng-packagr": "0.901.1",
    "@angular/cli": "9.1.9",
    "@angular/compiler-cli": "9.1.1",
    "@angular/language-service": "9.1.9",
    "@fourjs/tslint-rules": "2.2.0",
    "@types/jasmine": "3.5.12",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "14.0.27",
    "codacy-coverage": "3.4.0",
    "codelyzer": "6.0.1",
    "del": "^5.1.0",
    "gh-pages": "3.2.0",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-bump": "^3.2.0",
    "gulp-cssbeautify": "3.0.0",
    "gulp-cssimport": "7.0.0",
    "gulp-replace": "1.0.0",
    "gulp-sass": "4.1.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.1",
    "karma": "5.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "9.1.5",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslib": "1.13.0",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  }
}
