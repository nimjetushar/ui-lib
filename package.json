{
  "name": "ui-lib",
  "version": "0.0.1",
  "scripts": {
    "start": "ng serve",
    "build": "ng build ui-library && gulp",
    "build-demo": "ng build",
    "build-demo-prod": "ng build --prod=true",
    "test": "ng test",
    "test-lib": "ng test ui-library",
    "test-server": "ng test --watch=false --browsers=ChromeHeadless && ng test ui-library --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gulp": "gulp",
    "deploy": "npm run build && npm run build-demo-prod && gh-pages -d dist/ui-lib",
    "test-coverage": "ng test ui-library --codeCoverage=true --watch=false --browsers=ChromeHeadless && cat ./coverage/lcov.info | codacy-coverage -t 8863a82776d04472bf5173b2fe6c5f0b"
  },
  "author": "Tushar Nimje",
  "license": "MIT",
  "engines": {
    "node": ">=10.10.0",
    "npm": ">=6.6.0"
  },
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/cdk": "^8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "core-js": "^3.1.4",
    "font-awesome": "^4.7.0",
    "grid-styled-css": "^1.0.1",
    "ngx-toastr": "^10.0.2",
    "normalize.css": "^8.0.1",
    "primeicons": "^2.0.0",
    "primeng": "^8.0.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "tutility": "^1.0.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.0",
    "@angular-devkit/build-ng-packagr": "~0.803.21",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.6.9",
    "codacy-coverage": "3.4.0",
    "codelyzer": "^5.0.1",
